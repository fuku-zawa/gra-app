-# .status-wrapper
.status-bar
  .status-white-left
    =image_tag "status/W-Time Style.svg"
  .status-white-right
    - if user_signed_in?
      = link_to "ログアウト", destroy_user_session_path, data: {method: "delete"}
    =image_tag "status/W-Mobile Signal.svg", class: "status-bar-item"
    =image_tag "status/W-Wifi.svg", class: "status-bar-item"
    =image_tag "status/W-Battery.svg", class: "status-bar-item"